# mobileweb properties
PER_PROFILE			= [absolute_path_to_firefox_profile_used_by_selenium_rc]
hostname   			= [ip_address_of_selenium_rc_server_host]
portnumber 			= [port_number_of_selenium_rc_server_host]
explorer-adv		= *chrome [absolute_path_to_firefox_executable]
explorer-std		= *chrome [absolute_path_to_firefox_executable]
url        			= [url_of_mobileweb_application_under_test]
url.dotcom        	= [url_of_dotcom_application_under_test_for_redirection]
url.dotcom.redirect	= [url_that_dotcom_application_under_test_for_redirection_will_redirect_to]
url.infogin.redirect		= [url_of_infogin_site_under_test]
# project properties
Project.root 		= [absolute_path_to_project_root]
Project.platform 	= [windows_or_linux]



